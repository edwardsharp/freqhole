{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "freqhole",
  "version": "0.1.0",
  "identifier": "net.freqhole.app",
  "build": {
    "beforeDevCommand": {
      "cwd": "../app-solid",
      "script": "npm run dev"
    },
    "beforeBuildCommand": {
      "cwd": "../app-solid",
      "script": "npm run build"
    },
    "devUrl": "http://localhost:1420",
    "frontendDist": "../app-solid/dist"
  },
  "app": {
    "windows": [
      {
        "title": "freqhole",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
